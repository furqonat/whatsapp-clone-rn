{
    "cli": {
        "version": ">= 2.6.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "android": {
                "buildType": "apk"
            }
        },
        "release": {
            "autoIncrement": true,
            "android": {
                "buildType": "app-bundle"
            }
        }
    },
    "submit": {
        "production": {
            "android": {
                "track": "production",
                "serviceAccountKeyPath": "./service-account.json",
                "releaseStatus": "completed"
            }
        }
    }
}